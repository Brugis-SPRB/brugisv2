            {
               "name":"MeasureResults",
               "cfg":{
                  "closeGlyph":"1-close",
                  "withPanelAsContainer":false
               }
            },
            {
               "name":"Settings",
               "hideFrom":[
                  "Toolbar",
                  "DrawerMenu"
