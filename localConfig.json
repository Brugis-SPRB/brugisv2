               "cfg":{
                  "wrapWithPanel":false,
                  "closeGlyph":"1-close",
                  "overrideSettings":{
                     "history":false
                  },
                  "wrap":true
               }
            },
            { "name" : "MousePosition" },
            {
               "name":"Locate",
               "cfg":{
                  "glyph":"1-position-1",
                  "btnConfig":{
                     "className":"square-button"
                  }
               },
               "override":{
                  "Toolbar":{
                     "alwaysVisible":true
                  }
               }
            },
            { "name" : "ScaleBox" },
            {
               "name":"DrawerMenu",
               "cfg":{
                  "glyph":"1-stilo",
                  "buttonStyle":"primary",
                  "buttonClassName":"square-button",
                  "singleSection":true,
                  "menuOptions" : {
                      "show" : true,
                      "overlapMap" : false,
                      "width" : 300
                  }
               }
            },
            {
